{
  "id": "dh-image-importer",
  "title": "Daggerheart Image Replacer",
  "description": "Replaces images in Compendium with matching images from a directory",
  "authors": [
    {
      "name": "Ciryc"
    }
  ],
  "version": "1.0.1",
  "compatibility": {
    "minimum": "13",
    "verified": "13.347",
    "maximum": "13"
  },
  "relationships": {
    "requires": [
      {
        "id": "daggerheart",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/Foundryborne/daggerheart/refs/heads/main/system.json",
        "compatibility": {
          "minimum": "1.0.0",
          "verified": "1.0.1"
        }
      }
    ]
  },
  "esmodules": [
    "scripts/dh-image-importer.js"
  ],
  "templates": [
    "templates/custom-sheet.html",
    "templates/zip-upload.html",
    "templates/image-replacer.html"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "socket": false,
  "type": "module",
  "url": "https://github.com/luthes/dh-image-importer",
  "manifest": "https://github.com/luthes/dh-image-importer/releases/download/latest/module.json",
  "download": "https://github.com/luthes/dh-image-importer/releases/download/latest/dh-image-importer.zip",
  "license": "MIT",
  "readme": "README.md",
  "bugs": "https://github.com/luthes/dh-image-importer/issues",
  "changelog": "CHANGELOG.md"
}
